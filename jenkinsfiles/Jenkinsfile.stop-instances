elifePipeline {
    def ciStacks = ['api-dummy--ci', 'elife-bot--ci', 'elife-dashboard--ci', 'elife-metrics--ci', 'journal--ci', 'journal-cms--ci', 'lax--ci', 'medium--ci', 'pattern-library--ci', 'search--ci']
